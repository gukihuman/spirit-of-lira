{
  "prisma.fileWatcher": false,
  "files.exclude": {
    ".nuxt": true,
    ".vscode": true,
    "node_modules": true,
    "tests": true,
    ".gitignore": true,
    "package-lock.json": true,
    "prisma": true,
    "server": true,
    "_old": true
  },
  "workspace.isHidden": true,
  "folder-path-color.folders": [
    {
      "path": "systems",
      "color": "blue"
    },
    {
      "path": "components",
      "color": "magenta"
    },
    {
      "path": "data",
      "color": "red"
    },
    {
      "path": "logic",
      "color": "cyan"
    },
    {
      "path": "interface",
      "color": "green"
    },
    {
      "path": "entities",
      "color": "yellow"
    }
  ]
}